{
  "name": "omise.js",
  "version": "2.5.4",
  "description": "JavaScript library for collecting payment details on a user's browser and securely transmitting them to our servers",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --hot --colors --progress --config webpack.dev.js",
    "dev:production": "webpack-dev-server --hot --colors --progress --config=./webpack.prod.js",
    "build:dev": "NODE_ENV=development webpack -p --config=./webpack.dev.js",
    "build:staging": "NODE_ENV=development webpack -p --config=./webpack.staging.js",
    "build:production": "NODE_ENV=production webpack -p --config=./webpack.prod.js",
    "test": "NODE_ENV=test NODE_PATH=./:./src/ jest --no-cache --setupFiles dotenv/config",
    "test:watch": "NODE_ENV=test npm test -- --watch",
    "cypress": "cypress",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "precommit": "lint-staged && npm test",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "verbose": true
  },
  "keywords": [
    "Omise.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/omise/omise.js.git"
  },
  "author": "Omise <support@omise.co> (https://omise.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omise/omise.js/issues"
  },
  "homepage": "https://www.omise.co/omise-js",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.11.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.0.1",
    "clean-webpack-plugin": "^0.1.9",
    "cypress": "^3.8.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^3.10.2",
    "file-loader": "^0.8.5",
    "husky": "^0.14.3",
    "jest": "24.7.1",
    "lint-staged": "^7.2.0",
    "moment": "^2.28.0",
    "prettier-eslint-cli": "^5.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^2.2.0",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.3",
    "whatwg-fetch": "^2.0.3"
  }
}
